{
  "baud": 115200,
  "apply_to_layers": ["RAM", "BBR"], // Options: RAM, BBR (battery backup RAM), flash, default?
  "verify_layer": "RAM",
  "register_csv": "../initialize/ZED-F9T_Registers.csv",

  // Chip-specific configuration information
  "chips": [
    {
      "server_connection_target": "localhost:50051",    // TODO: get this from daq_config.json + network_config.json
      "uid": "DEADBEEF",                           // TODO: replace with real value
      "device": "/dev/ttyACM0",                         // TODO: replace with real device file
      "position": {
        "format": "LLH",              // "LLH" or "ECEF"
        "lat_deg": 37.4219999,        // degrees
        "lon_deg": -122.0840575,      // degrees
        "height_m": 12.345,           // meters (ellipsoidal)
        "acc_m": 0.02                 // 3D accuracy estimate (meters) for fixed mode
      }
    }
  ],


  "cfg_key_settings": [
    // --- Constellations & signals ---
    { "key": "CFG_SIGNAL_GPS_ENA", "value": 1 },
    { "key": "CFG_SIGNAL_GPS_L1CA_ENA", "value": 1 },
    { "key": "CFG_SIGNAL_GPS_L2C_ENA", "value": 1 },
    { "key": "CFG_SIGNAL_GPS_L5_ENA", "value": 0 },

    { "key": "CFG_SIGNAL_GAL_ENA", "value": 1 },
    { "key": "CFG_SIGNAL_GAL_E1_ENA", "value": 1 },
    //{ "key": "CFG_SIGNAL_GAL_E5A_ENA", "value": 1 },
    { "key": "CFG_SIGNAL_GAL_E5B_ENA", "value": 1 },

    { "key": "CFG_SIGNAL_BDS_ENA", "value": 1 },
    { "key": "CFG_SIGNAL_BDS_B1_ENA", "value": 1 },
    { "key": "CFG_SIGNAL_BDS_B2_ENA", "value": 1 },
    { "key": "CFG_SIGNAL_BDS_B1C_ENA", "value": 0 },
    { "key": "CFG_SIGNAL_BDS_B2A_ENA", "value": 0 },

    { "key": "CFG_SIGNAL_SBAS_ENA", "value": 0 },
    { "key": "CFG_SIGNAL_QZSS_ENA", "value": 0 },
    { "key": "CFG_SIGNAL_QZSS_L1CA_ENA", "value": 0 },
    { "key": "CFG_SIGNAL_QZSS_L2C_ENA", "value": 0 },
    { "key": "CFG_SIGNAL_QZSS_L5_ENA", "value": 0 },
    { "key": "CFG_SIGNAL_NAVIC_ENA", "value": 0 },
    { "key": "CFG_SIGNAL_NAVIC_L5_ENA", "value": 0 },

    // --- Timepulse global modes (use frequency + ratio) ---
    { "key": "CFG_TP_PULSE_DEF", "value": 1 },          // 1 = FREQ mode
    { "key": "CFG_TP_PULSE_LENGTH_DEF", "value": 0 },   // 0 = RATIO (percent)
    { "key": "CFG-TP-ANT_CABLEDELAY", "value": 10 },   // Antenna delay due to cable (in ns)

    // --- TP1: 1 Hz, 100 Âµs duty (=0.01%), GPS grid, align to TOW ---
    { "key": "CFG_TP_TP1_ENA", "value": 1 },
    { "key": "CFG_TP_FREQ_TP1", "value": 1 },
    { "key": "CFG_TP_DUTY_TP1", "value": 0.01 },
    { "key": "CFG_TP_SYNC_GNSS_TP1", "value": 1 },
    { "key": "CFG_TP_USE_LOCKED_TP1", "value": 1 },
    { "key": "CFG_TP_ALIGN_TO_TOW_TP1", "value": 1 },
    { "key": "CFG_TP_TIMEGRID_TP1", "value": 0 },       // 1 = GPS //0 = UTC
    { "key": "CFG_TP_FREQ_LOCK_TP1", "value": 1 },
    { "key": "CFG_TP_DUTY_LOCK_TP1", "value": 0.01 },

    // UBX-TIM-TP (qErr) at 1 Hz on USB and/or UART1
    { "key": "CFG_MSGOUT_UBX_TIM_TP_USB", "value": 1 },
    { "key": "CFG_MSGOUT_UBX_TIM_TP_UART1", "value": 1 },
    { "key": "CFG_MSGOUT_UBX_NAV_TIMEUTC_USB",   "value": 1 },
    { "key": "CFG_MSGOUT_UBX_NAV_TIMEUTC_UART1", "value": 1 },

    // --- TP2: 10 MHz, 50% duty ---
    { "key": "CFG_TP_TP2_ENA", "value": 1 },
    { "key": "CFG_TP_FREQ_TP2", "value": 10000000 },
    { "key": "CFG_TP_DUTY_TP2", "value": 50.0 },
    { "key": "CFG_TP_SYNC_GNSS_TP2", "value": 1 },
    { "key": "CFG_TP_TIMEGRID_TP2", "value": 0 }
  ]
}
